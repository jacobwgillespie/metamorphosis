{
  "name": "metamorphosis",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Jacob Gillespie <jacobwgillespie@gmail.com>",
  "main": "./dist/metamorphosis.cjs.js",
  "module": "./dist/metamorphosis.es.js",
  "types": "./dist/metamorphosis.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "prettier": {
    "bracketSpacing": false,
    "parser": "typescript",
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "bili",
    "test": "tsc",
    "watch": "bili --watch"
  },
  "dependencies": {
    "sinek": "^6.23.4"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "bili": "^3.4.2",
    "prettier": "^1.15.3",
    "rollup-plugin-prettier": "^0.5.0",
    "rollup-plugin-typescript2": "^0.18.1",
    "typescript": "^3.2.2"
  },
  "bili": {
    "input": [
      "src/metamorphosis.ts"
    ],
    "format": [
      "cjs",
      "es"
    ],
    "banner": true,
    "target": "node:10",
    "pretty": true
  }
}
